{
  "name": "abap-value-help",
  "version": "2.5.0",
  "description": "ABAP Value Help",
  "keywords": [
    "sap",
    "sapcloudplatform",
    "abap",
    "rfc",
    "sapnwrfc",
    "nwrfcsdk",
    "node",
    "nodejs"
  ],
  "homepage": "https://github.com/sap/fundamental-tools",
  "license": "Apache-2.0",
  "author": "SAP",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "files": [
    "package.json",
    "dist"
  ],
  "scripts": {
    "build": "reuse lint && rm -rf dist && npm run ts && npm run lint",
    "dep": "npm i --save loglevel node-rfc",
    "devDep": "npm i --save-dev @types/node @typescript-eslint/eslint-plugin @typescript-eslint/parser eslint jest typescript",
    "lint": "eslint src/ts",
    "test": "jest tests --detectOpenHandles",
    "ts": "tsc -p src/ts"
  },
  "dependencies": {
    "loglevel": "^1.8.1",
    "node-rfc": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "cpu": [
    "!arm"
  ]
}
