{
  "name": "abap-value-help",
  "description": "ABAP Value Help",
  "version": "1.0.1",
  "homepage": "https://github.com/sap/fundamental-tools",
  "author": "SAP",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 14.0"
  },
  "cpu": [
    "!arm"
  ],
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "type": "commonjs",
  "scripts": {
    "ts": "tsc -p src/ts",
    "lint": "eslint src/ts",
    "dep": "npm i --save loglevel node-rfc",
    "devDep": "npm i --save-dev @types/node  @typescript-eslint/eslint-plugin @typescript-eslint/parser eslint jest typescript",
    "build": "reuse lint && rm -rf dist && npm run ts && npm run lint",
    "lock": "npm install --package-lock-only",
    "test": "jest tests --detectOpenHandles"
  },
  "keywords": [
    "sap",
    "sapcloudplatform",
    "abap",
    "rfc",
    "sapnwrfc",
    "nwrfcsdk",
    "node",
    "nodejs"
  ],
  "devDependencies": {
    "@types/node": "^18.7.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "jest": "^28.1.3",
    "typescript": "^4.7.4"
  }
}
